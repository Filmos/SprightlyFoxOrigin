{
  "type":"origins:multiple",
  "full_regen": {
    "type": "origins:action_over_time",
    "interval": 3,
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "sprightly_fox:sprightly",
      "change": 1
    },
    "condition": {
      "type": "origins:food_level",
      "comparison": ">=",
      "compare_to": 16
    }
  },
  "half_regen": {
    "type": "origins:action_over_time",
    "interval": 6,
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "sprightly_fox:sprightly",
      "change": 1
    },
    "condition": {
      "type":"origins:and",
      "conditions":
      [
        {
          "type": "origins:food_level",
          "comparison": "<",
          "compare_to": 16
        },
        {
          "type": "origins:food_level",
          "comparison": ">=",
          "compare_to": 10
        }
      ]
    }
  },
  "low_regen": {
    "type": "origins:action_over_time",
    "interval": 9,
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "sprightly_fox:sprightly",
      "change": 1
    },
    "condition": {
      "type": "origins:food_level",
      "comparison": "<",
      "compare_to": 10
    }
  },
  "hidden": true
}